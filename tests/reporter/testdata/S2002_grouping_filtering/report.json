{
  "handle": "testing_report",
  "sources": [{
    "name": "unnamed_src",
    "groups": [
      {
        "name": "unnamed_grp",
        "steps": [
          { "load": {
            "name": "users",
            "source": "composeRecords",
            "definition": {
              "module": "user",
              "namespace": "ns"
            }
          }},

          { "group": {
            "name": "grouped",
            "source": "users",
            "keys": [
              { "name": "by_name", "column": "first_name" }
            ],
            "columns": [
              { "name": "count", "aggregate": "count" },
              { "name": "total", "aggregate": "sum", "column": "number_of_numbers" }
            ]
          }}
        ]
      }
    ]
  }],
  "frames": [{
    "name":   "result",
    "source": "grouped",
    "columns": [
      { "name": "by_name", "label": "by_name" },
      { "name": "count", "label": "count" },
      { "name": "total", "label": "total" }
    ],
    "sort": "by_name ASC",

    "rows": {
      "and": [{
        "cells": {
          "total": {"op": "gt", "value": "50"},
          "count": {"op": "lt", "value": "2"}
        }
      }]
    }
  }]
}
